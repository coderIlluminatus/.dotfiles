!! font styling 
Xft.dpi:                108
Xft.antialias:          true      
Xft.hinting:            true
Xft.rgba:               rgb 
Xft.autohint:           false
Xft.hintstyle:          hintfull
Xft.lcdfilter:          lcddefault

!! ___________________________________________________________________________________________
!! URxvt config
!! ___________________________________________________________________________________________

URxvt*scrollBar_right:  false
URxvt*scrollBar:        false
URxvt*borderLess:       false

#define terminal_font Inconsolata for Powerline
#define size 18

URxvt*font:             xft:terminal_font:bold:pixelsize=size:antialias=true:autohint=true
URxvt*boldFont:         xft:terminal_font:bold:pixelsize=size:antialias=true:autohint=true
URxvt*imFont:           xft:terminal_font:bold:pixelsize=size:antialias=true:autohint=true
URxvt*italicFont:       xft:terminal_font:regular:italic:pixelsize=size:antialias=true:autohint=true
URxvt*boldItalicFont:   xft:terminal_font:bold:italic:pixelsize=size:antialias=true:autohint=true

URxvt.cursorBlink:      true

! Extensions
URxvt.urlLauncher:      xdg-open
URxvt.urlButton:        1
URxvt.underlineURLs:    True
URxvt.matcher.button:   1

URxvt.perl-ext-common:  matcher,readline
URxvt.perl-lib:         $HOME/.urxvt/ext

! Keybindings
URxvt.keysym.Mod4-c: perl:clipboard:copy
URxvt.keysym.Mod4-v: perl:clipboard:paste
URxvt.keysym.Mod4-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.C-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u: perl:url-select:select_next